
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayoutPage.cshtml";
}


<div class="four-grids">
    <div class="col-md-3 four-grid">
        <div class="four-agileits" style="background-color:brown">
            <div class="icon">
                <i class="glyphicon fa fa-users" aria-hidden="true"></i>
            </div>
            <div class="four-text">
                <h3>Tổng số thành viên</h3>
                <h4>@ViewBag.TongSoThanhVien</h4>

            </div>

        </div>
    </div>
</div>
<div class="clearfix"></div>
<div class="four-grids">
    <div class="col-md-3 four-grid">
        <div class="four-agileits">
            <div class="icon">
                <i class="glyphicon fa fa-first-order" aria-hidden="true"></i>
            </div>
            <div class="four-text">
                <h3>Tổng số đơn hàng</h3>
                <h4>@ViewBag.TongSoDonHang</h4>

            </div>

        </div>
    </div>
    <div class="col-md-3 four-grid">
        <div class="four-agileinfo">
            <div class="icon">
                <i class="glyphicon fa fa-eye" aria-hidden="true"></i>
            </div>
            <div class="four-text">
                <h3>Số người truy cập</h3>
                <h4>@ViewBag.LuotTruyCap</h4>

            </div>

        </div>
    </div>
    <div class="col-md-3 four-grid">
        <div class="four-w3ls">
            <div class="icon">
                <i class="glyphicon fa fa-eye" aria-hidden="true"></i>
            </div>
            <div class="four-text">
                <h3>Đang truy cập</h3>
                <h4>@ViewBag.IsOnline</h4>

            </div>

        </div>
    </div>
    <div class="col-md-3 four-grid">
        <div class="four-wthree">
            <div class="icon">
                <i class="glyphicon fa fa-money" aria-hidden="true"></i>
            </div>
            <div class="four-text">
                <h3>Tổng doanh thu</h3>
                <h4>@ViewBag.TongDoanhThu.ToString("#,##") VND</h4>

            </div>

        </div>
    </div>
    <div class="clearfix"></div>
</div>



